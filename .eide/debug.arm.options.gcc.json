{
    "version": 5,
    "beforeBuildTasks": [],
    "afterBuildTasks": [],
    "global": {
        "$float-abi-type": "hard",
        "output-debug-info": "enable",
        "misc-control": "-pipe -fno-exceptions --specs=nano.specs -u _printf_float -u _scanf_float -flto=16"
    },
    "c/cpp-compiler": {
        "language-c": "gnu11",
        "language-cpp": "gnu++17",
        "optimization": "level-3",
        "warnings": "all-warnings",
        "CXX_FLAGS": "-fno-rtti -Wno-psabi"
    },
    "asm-compiler": {},
    "linker": {
        "output-format": "elf",
        "LIB_FLAGS": "-lm -lstdc++",
        "LD_FLAGS": "-Wl,--no-warn-rwx-segments"
    }
}